} { {System.out.println(Thread.currentThread().getStackTrace()[1].getMethodName());}
    n1=add() ((t=<GE> | t=<LE> | t=<GT> | t=<LT>) n2=add() {n1 = new BinOpNode(t.toString(), n1, n2);})?
    {return n1;}
}

