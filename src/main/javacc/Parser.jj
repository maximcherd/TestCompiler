PARSER_BEGIN(Parser)

import ast.*;
import semanticBase.*;
import printer.*;
import java.io.InputStream;import java.util.List;
import java.util.ArrayList;

public class Parser {
    public static void main(String args[]) {
        try {
            InputStream is = System.in;
            System.out.println("Creating parser...");
            StmtListNode node = new Parser(is).start();
            System.out.println("Syntax is okay");
            System.out.println(Printer.printTree(node.tree(), "\n"));
        } catch (Throwable e) {
            // Catching Throwable is ugly but JavaCC throws Error objects!
            System.out.println("Syntax check failed: " + e.getMessage());
        }
    }
}

PARSER_END(Parser)

SKIP:  {
//    < ~[] > |
    " " |
    "\t" |
    "\n" |
    "\r" |
    < C_COMMENTS: "/*" ( ~["*"] | "*" ~["/"] )* "*/"> |
    < SLASH_COMMENTS: "//" ("\\\n" | ~["\n"] )* >
}

TOKEN: {
    < EMPTY: "" > |
    < IF: "if" > |
    < ELSE: "else" > |
    < FOR: "for" > |
    < WHILE: "while" > |
    < RETURN: "return" > |
    < KEYWORD: <IF> | <ELSE> | <FOR> | <WHILE> | <RETURN> > |
    < NUM: (["+","-"])? (["0"-"9"])+ ("." (["0"-"9"])+)? (["e","E"] (["+","-"])? (["0"-"9"])+)? > |
    < STR: "\"" (~["\""])* "\"" > |
    < TRUE: "true" > | < FALSE: "false" > |
    < LITERAL: <NUM> | <STR> | <TRUE> | <FALSE> > |
    < TYPE: "void" | "int" | "float" | "string" | "bool" > |
    < IDENT: (["a"-"z","A"-"Z","_"]) (["a"-"z","A"-"Z","_","0"-"9"])* > |
    < LPAR: "(" > | < RPAR: ")" > |
    < LBRACK: "[" > | < RBRACK: "]" > |
    < LBRACE: "{" > | < RBRACE: "}" > |
    < SEMI: ";" > | < COMMA: "," > |
    < ASSIGN: "=" > |
    < ADD: "+" > | < SUB: "-" > |
    < MUL: "*" > | < DIV: "/" > | < MOD: "%" > |
    < AND: "&&" > | < OR: "||" > |
    < BIT_AND: "&" > | < BIT_OR: "|" > |
    < GE: ">=" > | < LE: "<=" > |
    < GT: ">" > | < LT: "<" > |
    < NEQUALS: "!=" > | < EQUALS: "==" >
}

public StmtListNode start(): {
    StmtListNode n;
} { {System.out.println(Thread.currentThread().getStackTrace()[1].getMethodName());}
    n=program()
    {return n;}
}

StmtListNode program(): {
    StmtListNode n;
} { {System.out.println(Thread.currentThread().getStackTrace()[1].getMethodName());}
    n=stmtList() <EOF>
    {System.out.println("...end of program");}
    {return n;}
}

LiteralNode num(): {
    Token t;
} { {System.out.println(Thread.currentThread().getStackTrace()[1].getMethodName());}
    t=<NUM>
    {return new LiteralNode(t.toString());}
}

