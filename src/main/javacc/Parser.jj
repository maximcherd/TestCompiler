} { {System.out.println(Thread.currentThread().getStackTrace()[1].getMethodName());}
    n1=compare1() ((t=<EQUALS> | t=<NEQUALS>) n2=compare1() {n1 = new BinOpNode(t.toString(), n1, n2);})?
    {return n1;}
}

